 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div style=\"font-family: Arial, sans-serif; max-width: 900px; margin: 0 auto; border: 1px solid #000;\">\n    <h2 style=\"text-align: center; margin: 20px 0 10px 0; font-size: 24px; letter-spacing: 1px;font-weight:bold;\">Exit Form</h2>\n    {% set employee = frappe.get_doc(\"Employee\", doc.employee) %}\n    <table style=\"width: 100%;margin-bottom: 0px; font-size: 12px;\">\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000; width: 20%;border-left:none;border-left:none;\"><b>Emp. Code</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000; width: 30%;\">{{ employee.employee_number or \"\" }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000; width: 20%;\"><b>Employee Name</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000; width: 30%;border-right:none;\">{{ doc.employee_name or \"\" }}</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;border-left:none;border-left:none;\"><b>Date of Joining</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\">{{ frappe.utils.format_date(doc.date_of_joining,\"dd-MM-yyyy\") or \"\" }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"><b>Resignation Date</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\">{{ frappe.utils.format_date(employee.resignation_letter_date,\"dd-MM-yyyy\") or \"\" }}</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;border-left:none;border-left:none;\"><b>Department</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\">{% if doc.department %}\n                {{ frappe.db.get_value(\"Department\",doc.department,\"department_name\") or \"\" }}\n                {% endif %}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"><b>Last Working Day</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\">{{ frappe.utils.format_date(doc.relieving_date,\"dd-MM-yyyy\") or \"\" }}</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;border-left:none;\"><b>Designation</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\">{% if doc.designation %}\n                {{ frappe.db.get_value(\"Designation\",doc.designation,\"designation_name\") or \"\" }}\n                {% endif %}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"><b>Personal E-Mail ID</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\">{{ employee.personal_email or \"\" }}</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;border-left:none;\"><b>Reporting to</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\">{{ doc.reports_to or \"\" }}</td>\n            <td rowspan=\"2\" style=\"padding: 6px; border: 1px solid #000;vertical-align:middle !important;\"><b>Contact Address</b></td>\n            <td rowspan=\"2\" style=\"padding: 6px; border: 1px solid #000;border-right:none;vertical-align:middle !important;\">{%- set address_parts = [] -%}\n{%- if employee.custom_current_address_line_1 %}{% set _ = address_parts.append(employee.custom_current_address_line_1) %}{% endif -%}\n{%- if employee.custom_current_address_line_2 %}{% set _ = address_parts.append(employee.custom_current_address_line_2) %}{% endif -%}\n{%- if employee.custom_current_city %}{% set _ = address_parts.append(employee.custom_current_city) %}{% endif -%}\n{%- if employee.custom_current_state %}{% set _ = address_parts.append(employee.custom_current_state) %}{% endif -%}\n{%- if employee.custom_current_zip_code %}{% set _ = address_parts.append(employee.custom_current_zip_code) %}{% endif -%}\n{{ address_parts | join(', ') }}\n</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;border-left:none;\"><b>Tel. / Mob. No</b></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\">{{ employee.custom_preferred_mobile_no or \"\" }}</td>\n        </tr>\n    </table>\n\n    <div style=\"background: #bcbcbc; text-align: center; padding: 8px 0; font-weight: bold; font-size: 16px;\">\n        CLEARANCE AUTHENTICATION\n    </div>\n\n    <table style=\"width: 100%; border-collapse: collapse; font-size: 12px;\">\n        <tr>\n            <th style=\"padding: 6px; border: 1px solid #000; width: 50%;text-align:center;color:black !important; font-weight:bold;border-left:none;\">Particulars</th>\n            <th style=\"padding: 6px; border: 1px solid #000; width: 20%;text-align:center;color:black !important; font-weight:bold;\">Pending</th>\n            <th style=\"padding: 6px; border: 1px solid #000; width: 15%;text-align:center;color:black !important; font-weight:bold;\">Received</th>\n            <th style=\"padding: 6px; border: 1px solid #000; width: 15%;text-align:center;color:black !important; font-weight:bold;border-right:none;\">Signature</th>\n        </tr>\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;\">Stores</td>\n        </tr>\n        {% set stores = [\n            \"Spare Parts\", \"Bioeasy Kit\", \"Milkoscreen Kit\", \"High End Kit\"\n        ] %}\n        {% for item in stores %}\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;\">{{ item }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-right:none;\"></td>\n        </tr>\n        {% endfor %}\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;\">Admin</td>\n        </tr>\n        {% set admin = [\n            \"Service Bag\", \"Uniform & Tshirt\", \"ID Card\", \"Visiting Card\", \"Stationery Items\", \"SIM Card\", \"OYO ID\"\n        ] %}\n        {% for item in admin %}\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;border-left:none;\">{{ item }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\"></td>\n        </tr>\n        {% endfor %}\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;text-align:center;\">Accounts</td>\n        </tr>\n        {% set accounts = [\n            \"Outstanding Advances\", \"Imprest Recovery\", \"Relocation Advances\", \"Any Other Deduction\"\n        ] %}\n        {% for item in accounts %}\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;\">{{ item }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\"></td>\n        </tr>\n        {% endfor %}\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;\">Coordinator</td>\n        </tr>\n        {% set coordinator = [\n            \"TADA\", \"Hard Copy Submitted\"\n        ] %}\n        {% for item in coordinator %}\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;border-left:none;\">{{ item }}</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        {% endfor %}\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;\">IT</td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;\">Laptop</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;\">Laptop Bag </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;text-align:center;border-left:none;border-left:none;\">Laptop Charger </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;text-align:center;border-left:none;\">Mouse </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;text-align:center;border-left:none;\">Dongle</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td colspan=\"4\" style=\"padding: 6px; background: #bcbcbc; font-weight: bold;text-align:center;text-align:center;border-left:none;\">Profile handover </td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;\">Knowledge </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;\">Data </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;\">Pending Task</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;\">Contact Person  </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;border-left:none;\">Client List</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\"></td>\n        </tr>\n                <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;\">Vendor List </td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\"></td>\n        </tr>\n        <tr>\n            <td style=\"padding: 6px; border: 1px solid #000;text-align:center;text-align:center;border-left:none;border-left:none;border-left:none;border-left:none;\">CD</td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;\"></td>\n            <td style=\"padding: 6px; border: 1px solid #000;border-right:none;\"></td>\n        </tr>\n        <tr>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-left:none;border-left:none;border-left:none;border-left:none;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-right:none;\"></td>\n</tr>\n<tr>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-left:none;border-left:none;border-left:none;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-right:none;\"></td>\n</tr>\n<tr>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-left:none;border-left:none;border-left:none;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;\"></td>\n    <td style=\"padding: 6px; border: 1px solid #000; min-height: 28px; height: 28px;border-right:noneborder-right:none;border-right:none;border-right:none;;\"></td>\n</tr>\n\n    </table>\n    <!-- Deduction Summary Table -->\n<table style=\"width:100%; border-collapse:collapse; font-size:12px;\">\n    <tr style=\"background:#bcbcbc;\">\n        <th style=\"padding:6px; border:1px solid #000; width:20%;border-left:none;border-top:none;\"></th>\n        <th style=\"padding:6px; border:1px solid #000; width:10%;border-top:none;\"></th>\n         <th style=\"padding:6px; border:1px solid #000; width:20%;border-top:none;\"></th>\n        <th style=\"padding:6px; border:1px solid #000; width:20%;text-align:center;color:black !important; font-weight:bold;border-top:none;\">No.Of Days</th>\n        <th style=\"padding:6px; border:1px solid #000; width:15%;text-align:center;color:black !important;font-weight:bold;border-top:none;\">Amount</th>\n        <th style=\"padding:6px; border:1px solid #000; width:15%;text-align:center;color:black !important;font-weight:bold;border-right:none;border-top:none;\">Signature</th>\n    </tr>\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000;text-align:center;color:black !important; font-weight:bold;border-left:none;border-left:none;\" colspan=\"3\">LOP</td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n    </tr>\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000;text-align:center;border-left:none;\" colspan=\"3\">PL ( To be reimbursed if any )</td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n    </tr>\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000;text-align:center;text-align:center;border-left:none;\" colspan=\"3\">Short Notice Period</td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-right:none;border-right:none;border-right:none;\"></td>\n    </tr>\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000;text-align:center;text-align:center;border-left:none;\" colspan=\"3\">Any Other Deduction</td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-right:none;border-right:none;\"></td>\n    </tr>\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000;text-align:center;text-align:center;border-left:none;border-bottom:none;\" colspan=\"3\">Greytip</td>\n        <td style=\"padding:6px; border:1px solid #000;border-bottom:none;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-bottom:none;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-right:none;border-right:none;border-bottom:none;\"></td>\n    </tr>\n    <tr>\n    <tr>\n        <td style=\"min-height: 28px; height: 28px;padding:6px; border:1px solid #000;text-align:center;text-align:center;border-left:none;border-bottom:none;\" colspan=\"3\"></td>\n        <td style=\"min-height: 28px; height: 28px;padding:6px; border:1px solid #000;border-bottom:none;\"></td>\n        <td style=\"padding:6px; border:1px solid #000;border-bottom:none;min-height: 28px;\"></td>\n        <td style=\"min-height: 28px;padding:6px; border:1px solid #000;border-right:none;border-right:none;border-bottom:none;\"></td>\n    </tr>\n</tr>\n</table>\n\n    <!-- IT INCHARGE Section -->\n<table style=\"width:100%;font-size:12px;\">\n    <colgroup>\n        <col style=\"width:35% !important;\">\n        <col style=\"width:20% !important;\">\n        <col style=\"width:20%;\">\n        <col style=\"width:15%;\">\n        <col style=\"width:15%;\">\n    </colgroup>\n    <tr>\n        <th colspan=\"5\" style=\"color:black !important; font-weight:bold;padding:6px; border:1px solid #000; background:#bcbcbc; text-align:center;border-right:none;border-left:none;\">IT INCHARGE</th>\n    </tr>\n    <tr style=\"background:#bcbcbc;\">\n        <th style=\"padding:6px; border:1px solid #000;text-align:center;text-align:center;color:black !important; font-weight:bold;border-left:none;\">IT Incharge</th>\n        <th style=\"padding:6px; border:1px solid #000;text-align:center;color:black !important; font-weight:bold;\">Discontinue</th>\n        <th style=\"padding:6px; border:1px solid #000;text-align:center;color:black !important; font-weight:bold;\">Hold Till<br>(Mention Date here)</th>\n        <th style=\"padding:6px; border:1px solid #000;text-align:center;color:black !important; font-weight:bold;\">Forward/Transfer To</th>\n        <th style=\"padding:6px; border:1px solid #000;text-align:center;color:black !important; font-weight:bold;border-right:none;\">Signatures<br>(to whom transferred)</th>\n    </tr>\n    {% set it_items = [\n        \"Email ID\", \"CRM ID\", \"BTS Id\", \"Zoho\", \"Outlook Uninstalled\", \"WhatsApp Uninstalled\", \"Call forwarding has been deactivate\"\n    ] %}\n    {% for item in it_items %}\n    <tr>\n        <td style=\"padding:6px; border:1px solid #000; min-height:28px; height:28px;text-align:center;border-left:none;\">{{ item }}</td>\n        <td style=\"padding:6px; border:1px solid #000; min-height:28px; height:28px;\"></td>\n        <td style=\"padding:6px; border:1px solid #000; min-height:28px; height:28px;\"></td>\n        <td style=\"padding:6px; border:1px solid #000; min-height:28px; height:28px;\"></td>\n        <td style=\"padding:6px; border:1px solid #000; min-height:28px; height:28px;border-right:none;\"></td>\n    </tr>\n    {% endfor %}\n</table>\n\n<!-- Declaration From Employee Section -->\n<table style=\"width:100%; border-collapse:collapse;font-size:12px;\">\n    <tr>\n        <th colspan=\"4\" style=\"padding:6px; border:1px solid #000; background:#bcbcbc; text-align:center; font-weight:bold;color:black !important;border-right:none;border-left:none;border-top:none;\">Declaration From Employee</th>\n    </tr>\n    <tr>\n        <td colspan=\"4\" style=\"padding:8px; border:1px solid #000; text-align:left;border-right:none;border-left:none;text-align:center;\">\n            I have carefully gone though the above details and found to be correct.\n        </td>\n    </tr>\n    <tr>\n        <td colspan=\"2